<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Web CMS Suite Sites Crawling" parallel="instances" data-provider-thread-count="6">
    <listeners>
        <listener class-name="util.WebDriverListener"></listener>
    </listeners>
    <test name="CheckCrawlingSite">
        <parameter name="browserName" value="chrome"></parameter>
        <classes>
            <class name="specs.PublicSite.CheckCrawlingSite" />
        </classes>
    </test>
</suite>